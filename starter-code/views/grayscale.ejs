<h2> GrayScale Colors </h2>

<ul>

  <% listOfColors.forEach((oneColor) => { %>
    <% if (oneColor.category === 'grayscale') { %>
    <li>
    <div class="div" style="background-color: <%= oneColor.hex %> ;" ></div>
    <p> <%= oneColor.keyword %> </p>
    <p> <%= oneColor.hex %> </p>
    </li>
  <% } %>
<% }); %>
</ul>
